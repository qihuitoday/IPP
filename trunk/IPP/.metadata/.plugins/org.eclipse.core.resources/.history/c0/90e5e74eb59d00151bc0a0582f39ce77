package com.qihui.ssh.service.site;

import java.util.List;

import com.qihui.ssh.dao.entities.Level;
import com.qihui.ssh.dao.entities.Region;
import com.qihui.ssh.dao.entities.Site;
import com.qihui.ssh.dao.site.SiteDAO;
import com.qihui.ssh.service.site.SiteService;

public class SiteServiceImpl implements SiteService {

	private SiteDAO siteDAO;

	public void addSite(Site site) {
		siteDAO.addSite(site);
	}

	public List<Region> getAllProvince() {
		return siteDAO.getAllProvince();
	}

	public List<Level> getAllLevel() {
		return siteDAO.getAllLevel();
	}

	@Override
	public List<Site> getSite() {
		return siteDAO.getSite();
	}

	public void setSiteDAO(SiteDAO siteDAO) {
		this.siteDAO = siteDAO;
	}

}
